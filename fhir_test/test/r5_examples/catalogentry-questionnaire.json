          "linkId": "CatalogEntry.implicitRules",
          "type": "group",
          "required": false,
          "repeats": true,
          "item": [
            {
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-itemControl",
